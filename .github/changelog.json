{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feat"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## 📝 Documentation",
      "labels": ["docs"]
    },
    {
      "title": "## 🔨 Maintenance",
      "labels": ["chore", "refactor"]
    },
    {
      "title": "## ⚡️ Performance",
      "labels": ["perf"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test"]
    }
  ],
  "ignore_labels": ["ignore"],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes"
}
